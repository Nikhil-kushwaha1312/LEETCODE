class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        vector<int> ans;
        int ct1=0,el1;
        int ct2=0,el2;
        for(int i=0;i<nums.size();i++){
            if(ct1==0 && nums[i]!=el2){
                ct1=1;
                el1=nums[i];
            }
            else if(ct2==0 && nums[i]!=el1){
                ct2=1;
                el2=nums[i];
            }
            else if(nums[i]==el1){
                ct1++;
            }
            else if(nums[i]==el2){
                ct2++;
            }
            else{
                ct1--;
                ct2--;
            }
        }
        ct1=0;
        ct2=0;
        int n=nums.size();
        int lim=(int)(n/3)+1;
        for(int i=0;i<nums.size();i++){
            if(el1==nums[i]) ct1++;
            if(el2==nums[i]) ct2++;
        }
        if(ct1>=lim) ans.push_back(el1);
        if(ct2>=lim && el2!=el1) ans.push_back(el2);
        return ans;
    }
};
