vector<int> tri(int n){
    long long num=1;
    vector<int> ans;
    ans.push_back(1);
    for(int i=1;i<n;i++){
        num=num*(n-i)/i;
        ans.push_back(num);
    }
    return ans;
}

class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans;
        for(int i=1;i<=numRows;i++){
            ans.push_back(tri(i));
        }
        return ans;
    }
};
