class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int ans=0;
        map<int,int>mpp;
        for(int i=0;i<nums.size();i++){
            mpp[nums[i]]+=1;
        }
        for(auto a:mpp){
            ans+=a.second*(a.second-1)/2;
        }
        return ans;
    }
};
