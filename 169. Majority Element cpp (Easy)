class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int ele;
        int count=0;
        for(int i=0;i<nums.size();i++){
            if(count==0){
                count=1;
                ele=nums[i];
            }else if(nums[i]==ele){
                count++;
            }else{
                count--;
            }
        }
        if(count >=1) return ele;
        else return -1;
    }
};
