class Solution {
public:
    bool isPalindrome(string s) {
        string k;
        for(char c:s){
            if(isalnum(c)){
                k+=tolower(c);
            }
        }
        int left=0,right =k.size()-1;
        while(left<right){
            if(k[left]!=k[right]) return false; 
            left++;
            right--;
            
        }
        return true;
    }
};
