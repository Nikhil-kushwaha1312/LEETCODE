void reversee(vector<int>& nums, int left, int right) {
    while (left < right) {
        int temp = nums[left];
        nums[left] = nums[right];
        nums[right] = temp;
        left++;
        right--;
    }
}
class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n = nums.size();
        k = k % n;
        reversee(nums, 0, n - 1);
        reversee(nums, 0, k - 1);
        reversee(nums, k , n - 1);
    }
};
